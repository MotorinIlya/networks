<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="200"
        x:Class="Snake.View.Create.CreateFieldWindow"
        Title="Snake"
        Width="400"
        Height="400">
    <StackPanel Orientation="Vertical">
        <StackPanel Width="200" 
                    Orientation="Horizontal"
                    Margin="20">
            <TextBlock Text="Name"/>
            <TextBox x:Name="Name"/>
        </StackPanel>
        <StackPanel Width="200" 
                    Orientation="Horizontal"
                    Margin="20">
            <TextBlock Text="GameName"/>
            <TextBox x:Name="GameName"/>
        </StackPanel>
        <StackPanel Width="200" 
                    Orientation="Horizontal"
                    Margin="20">
            <TextBlock Text="Width(10-100)"/>
            <TextBox x:Name="Width"/>
        </StackPanel>
        <StackPanel Width="200" 
                    Orientation="Horizontal"
                    Margin="20">
            <TextBlock Text="Height(10-100)"/>
            <TextBox x:Name="Height"/>
        </StackPanel>
        <Button Click="ClickCreate">Start</Button>
    </StackPanel>
</Window>
